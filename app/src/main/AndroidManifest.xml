<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.orcs.boomshakalaka"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.library.DialogWidgetActivity"/>
        <activity android:name=".activity.library.SpinnerWidgetActivity"/>
        <activity android:name=".activity.library.InfoCenterActivity"/>
        <activity android:name=".activity.library.ExpanableListViewActivity"/>
        <activity android:name=".activity.library.WheelViewDemoActivity"/>

    </application>

</manifest>
